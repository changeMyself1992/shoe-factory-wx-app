.box {
  background-color: #fff;
  margin-top: 20rpx;
  padding: 0 20rpx;
}

.box .header {
  /* border-bottom: 2rpx solid #ccc; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}

.box .header .text-a {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 34rpx;
  /* width: 70%; */
}

.box .header .process-select {
  /* display: flex;
  align-items: center; */
  height: 60rpx;
  display: inline-block;
}

.box .header .process-select .key {
  margin-right: 10rpx;
  font-weight: 700;
}

.box .header .process-select .value {
}

.box .header .text-b {
  color: #d2767f;
}

.box .section {
  display: flex;
  flex-wrap: wrap;
  /* border-bottom: 2rpx solid #ccc; */
  align-items: center;
  padding: 10rpx 0;
}

.item {
}

.item .key {
  font-size: 24rpx;
}

.item .value {
  font-size: 24rpx;
  font-weight: 700;
}

.box .section .tag-row {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 4rpx;
  flex-wrap: wrap;
}

.box .section  .time-row {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 4rpx;
  flex-wrap: wrap;
}

.box .section  .time-row .item {
  /* margin: 0 20rpx; */
  flex: 1;
}

.box .section  .time-row .item.text-right {
  text-align: right;
}

.box .section  .time-row .item.beizhu {
  width: 100%;
}

.box .section .a-row {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.box .section .a-row .item {
  width: 60%;
  display: flex;
  align-items: center;
}

.box .section .a-row .item .key {
  width: 140rpx;
}

.box .section .a-row .item .value {
  width: 100%;
}

.box .footer {
  /* height: 80rpx; */
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding-bottom: 10rpx;
}

.box .footer .btn-consent {
  font-size: 26rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin: 0 10rpx;
  border-radius: 6rpx
}

.box .footer .btn-reject {
  font-size: 26rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin: 0;
}
