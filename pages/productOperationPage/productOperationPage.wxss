/*********row-2**********************start**************/

.row-2 {
  margin-bottom: 120rpx;
}

.row-2 .header {
  display: flex;
  justify-content: space-between;
  height: 80rpx;
  padding: 0 20rpx;
  align-items: center;
  background-color: #f8f7f5;
}

.row-2 .header .des {
  font-weight: 700;
}

.part-box {
  background-color: #fff;
  padding: 0 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-bottom: 20rpx;
  position: relative;
}

.part-box  .box-top {
  border-bottom: 2rpx solid #ccc;
  display: flex;
  width: 100%;
  justify-content: space-between;
  padding: 10rpx 0;
}

.part-box .part-name {
  width: 48%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  align-items: center;
}

.part-box .part-name .key {
  font-size: 24rpx;
}

.part-box .part-name .value {
  height: 40rpx;
  border: 2rpx solid #ccc;
  font-size: 24rpx;
  padding-left: 5px;
  width: 200rpx;
}

.part-box .part-name .suggest-value-list {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
}

.part-box .part-name .suggest-value-list .suggestion-box {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
  position: relative;
  margin: 5px 0;
  -webkit-box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  border-radius: 4px;
  border: 1px solid #e4e7ed;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  background-color: #fff;
}

.part-box .part-name .suggest-value-list .suggestion-box .tag {
  width: 100%;
  line-height: 50rpx;
  height: 50rpx;
  padding-left: 30rpx;
}

.part-box .clear {
  font-size: 24rpx;
  height: 50rpx;
  line-height: 50rpx;
  margin: 0;
}

.part-box .material-box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  border-bottom: 2rpx solid #ccc;
  width: 100%;
  padding: 20rpx 0 10rpx 0;
}

.part-box .material-box .item-material {
  width: 48%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  line-height: 40rpx;
  margin-bottom: 10rpx;
}

.part-box .material-box .item-material .key {
  font-size: 24rpx;
  white-space: nowrap;
  margin-right: 20rpx;
}

.part-box .material-box .item-material .value {
  height: 40rpx;
  border: 2rpx solid #ccc;
  font-size: 24rpx;
  padding-left: 5px;
  width: 200rpx;
}

.part-box .material-box .item-material .suggest-value-list {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
}

.part-box .material-box .item-material .suggest-value-list .suggestion-box {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
  position: relative;
  margin: 5px 0;
  -webkit-box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  border-radius: 4px;
  border: 1px solid #e4e7ed;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  background-color: #fff;
}

.part-box .material-box .item-material .suggest-value-list .suggestion-box .tag {
  width: 100%;
  line-height: 50rpx;
  height: 50rpx;
  padding-left: 30rpx;
}

.part-box .material-info {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 20rpx 0 20rpx 0;
  border-bottom: 2rpx solid #ccc;
}

.part-box .item-b {
  width: 48%;
  height: 60rpx;
  display: flex;
  align-items: center;
}

.part-box .item-b .key {
  font-size: 24rpx;
  white-space: nowrap;
  margin-right: 20rpx;
}

.part-box .item-b .value {
  border: 2rpx solid #ccc;
  padding-left: 10rpx;
  font-size: 24rpx;
}

.part-box .item-c {
  width: 100%;
  display: flex;
}

.part-box .item-c .key {
  font-size: 24rpx;
  white-space: nowrap;
  margin-right: 20rpx;
}

.part-box .item-c .value {
  border: 2rpx solid #ccc;
  height: 120rpx;
  padding-left: 10rpx;
  width: 100%;
}

.part-box .supplier-box {
  display: flex;
  flex-wrap: wrap;
  height: auto;
  padding-top: 10rpx;
  justify-content: space-between;
}

.part-box .supplier-box .item-supplier-name {
  padding-top: 4rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  width: 48%;
  justify-content: space-between;
}
.part-box .supplier-box .item-supplier-name .key {
  font-size: 24rpx;
  white-space: nowrap;
}
.part-box .supplier-box .item-supplier-name .value {
  border: 2rpx solid #ccc;
  height: 40rpx;
  width: 40%;
  padding-left: 10rpx;
  font-size: 24rpx;
}
.part-box .supplier-box .item-supplier-name .suggest-value-list{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
}

.part-box .supplier-box .item-supplier-name .suggest-value-list .suggestion-box {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
