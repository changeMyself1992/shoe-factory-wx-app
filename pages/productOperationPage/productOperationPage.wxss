  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  background-color: #fff;
}
.part-box .supplier-box .item-supplier-name .suggest-value-list .suggestion-box .tag {
  width: 100%;
  line-height: 50rpx;
  height: 50rpx;
  padding-left: 30rpx;
}

.part-box .supplier-box .item-supplier {
  height: 60rpx;
  display: flex;
  align-items: center;
  width: 48%;
}

.part-box .supplier-box .item-supplier .key {
  font-size: 24rpx;
  white-space: nowrap;
  margin-right: 20rpx;
}

.part-box .supplier-box .item-supplier .value {
  border: 2rpx solid #ccc;
  height: 40rpx;
  width: 100%;
  padding-left: 10rpx;
  font-size: 24rpx;
}

.part-box .supplier-box .item-supplier.name {
  width: 100%;
  flex-wrap: wrap;
  height: auto;
  justify-content: space-between;
  margin-bottom: 10rpx;
}

.part-box .supplier-box .item-supplier.name .value {
  width: 80%;
}

.part-box .supplier-box .item-supplier .suggest-value-list {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
}

.part-box .supplier-box .item-supplier .suggest-value-list .suggestion-box {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
  position: relative;
  margin: 5px 0;
  -webkit-box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
  border-radius: 4px;
  border: 1px solid #e4e7ed;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  background-color: #fff;
}

.part-box .supplier-box .item-supplier .suggest-value-list .suggestion-box .tag {
  width: 100%;
  line-height: 50rpx;
  height: 50rpx;
  padding-left: 30rpx;
}

.part-box .supplier-box .item-supplier.textarea {
  width: 100%;
  height: 110rpx;
}

.part-box .supplier-box .item-supplier.textarea .value {
  height: 80rpx;
}

/*********row-2**********************end**************/

.btn-area {
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 10;
  display: flex;
}

.btn-area button {
  margin: 0;
  border-radius: 0;
}

.btn-area button::after {
  border: none;
}

.btn-area button:first-child {
  width: 60%;
}

.btn-area button:last-child {
  width: 40%;
}
.form-item .id-card-front-photo image{
  height: 200rpx;
}
