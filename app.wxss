@import "./static/wxss/font1.wxss";
@import './static/wxss/global.wxss';

page {
  width: 100%;
  min-height: 100%;
  background-color: #f8f7f5;
}

.container {
  width: 100%;
  box-sizing: border-box;
  font-family: Microsoft YaHei, SimSun, NSimSun, PingFangSC-Light, helvetica, Heiti SC;
  margin: 0;
  padding: 0;
  min-height: 100%;
  overflow: hidden;
}

view, image, text, navigator {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

view, text {
  font-family: Microsoft YaHei, SimSun, NSimSun, PingFangSC-Light, helvetica, Heiti SC;
  font-size: 28rpx;
  color: #333;
}


